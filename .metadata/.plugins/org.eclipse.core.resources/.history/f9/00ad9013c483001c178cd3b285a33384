public class GerenciamentoCarro {

	public static void main(String[] args) {
		
		//objeto
		Carro c1 = new Carro("VW", "Gol", 2010, false, 0);
		Carro c2 = new Carro("BMW", "x", 2022);
		Carro c3 = new Carro("PEUGEOT", "207", 2019);
		
		c1.detalhes();
		c2.detalhes();
		c3.detalhes();
		

	}

}
